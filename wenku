<script>
    // 资格考试，生活休闲xpage配置
    // 渲染方法
    function render(resData) {
        var optData = {
            'content': '',
            'optarr': []
        };
        for (var i = 0; i < resData.length; i++) {
            // 容器的class,最后一个加last去除底部虚线
            var elClass = ((i + 1) === resData.length) ? 'tob-home-left last' : 'tob-home-left';
            // 定义容器
            var content = '<dl class="'
            + elClass
            + '"><dt><b class="t-tag cg"></b><a href="'
            + resData[i].url
            + '" target="_blank">'
            + resData[i].title
            + '<b class="li-aw n-ic"></b></a></dt><dd></dd></dl>';
            optData['content'] = optData['content'] + content;
            // 定义内容
            var optStr = '';
            var data = resData[i].options;
            for (var j = 0; j < data.length; j++) {
                optStr = optStr + '<a href="'
                + data[j].url
                + '" style="position:relative;" target="_blank">'
                + data[j].title
                + '<span class="ui-bz-'
                + data[j].type
                + '-ic" style="position: absolute;top: -11px;right: -20px;"></span></a>'
            }
            optData['optarr'].push(optStr);
        }
        // 插入容器
        $('.home-left-third').after(optData['content']);
        // 插入内容
        $('.tob-home-left').each(function (i, el) {
            $(this).find('dd').html(optData['optarr'][i]);
        });
    }
    // xpage获取数据
    $.ajax({
        url: '/xpage/form/getform?id=tob_home_left',
        success: function (data) {
            if (data.status.code === 0) {
                var model =  data.data.form_data && data.data.form_data.model;
                if (model && model.length > 0) {
                    render(model);
                }
            }
        },
        error: function () {
        }
    });
</script>
</div>
</div>
